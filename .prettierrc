{
	"$schema": "https://www.schemastore.org/prettierrc.json",
	"plugins": [
		"prettier-plugin-tailwindcss",
		"prettier-plugin-jinja-template"
	],
	"tailwindStylesheet": "./src/css/app.css",
	"htmlWhitespaceSensitivity": "ignore",
	"overrides": [
		{
			"files": [
				"src/**/*.html"
			],
			"options": {
				"parser": "jinja-template"
			}
		}
	]
}
